<part>
  <display-name>City list</display-name>
  <config>
    <input type="ComboBox" name="mapType">
      <label>Map type</label>
      <occurrences minimum="0" maximum="1"/>
      <config>
        <property name="option.value">ROADMAP</property>
        <property name="option.label">ROADMAP</property>

        <property name="option.value">SATELLITE</property>
        <property name="option.label">SATELLITE</property>

        <property name="option.value">HYBRID</property>
        <property name="option.label">HYBRID</property>

        <property name="option.value">TERRAIN</property>
        <property name="option.label">TERRAIN</property>
      </config>
    </input>
    <input type="TextLine" name="zoom">
      <label>Zoom level 1-15</label>
      <occurrences minimum="0" maximum="1"/>
    </input>
  </config>
</part>